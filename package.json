{
  "name": "RiveR",
  "version": "1.0.0",
  "description": "River Application to calculate River Discharge live from an Rowe Technologies, Inc. (RTI) ADCP.",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --colors",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "build:win": "electron-builder build --win --x64",
    "build:mac": "electron-builder build --mac --x64"
  },
  "build": {
    "appId": "com.rowetechinc.river",
    "productName": "RiveR",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "win": {
      "target": "nsis",
      "asar": false
    },
    "buildVersion": "1.0.0"
  },
  "author": "Rico Castelo @RTI",
  "license": "MIT",
  "dependencies": {
    "@date-io/core": "^2.0.0",
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.7.1",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/pickers": "^3.2.8",
    "@trendmicro/react-sidenav": "^0.4.5",
    "@types/date-fns": "^2.6.0",
    "@types/node": "^12.12.14",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/styled-components": "^4.4.0",
    "@types/vis": "^4.21.19",
    "bootstrap": "^4.4.1",
    "date-fns": "^2.9.0",
    "jquery": "^3.4.1",
    "node-gyp": "^6.1.0",
    "plotly.js": "^1.51.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-icons-kit": "^1.3.1",
    "react-plotlyjs-ts": "^2.1.0",
    "react-router-dom": "^5.1.2",
    "react-sidenav": "^4.0.6",
    "react-vis": "^1.11.7",
    "rebuild": "^0.1.2",
    "styled-components": "^4.4.1",
    "typeface-roboto": "0.0.75",
    "zerorpc": "fyears/zerorpc-node"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/plotly.js": "^1.44.17",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.5",
    "acorn": "^6.4.0",
    "ajv": "^6.10.2",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.1",
    "electron": "^4.2.12",
    "electron-builder": "^20.44.4",
    "electron-rebuild": "^1.8.8",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.15",
    "mocha": "^5.2.0",
    "node-loader": "^0.6.0",
    "rimraf": "^2.7.1",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^5.4.5",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-microsoft-contrib": "^5.2.1",
    "typescript": "^3.7.2",
    "webpack": "^4.28.1",
    "webpack-cli": "^3.3.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rowetechinc/River_electron"
  },
  "keywords": [
    "boilerplate",
    "Electron",
    "React",
    "Typescript",
    "Webpack",
    "Rowe Technologies, Inc.",
    "ADCP",
    "Discharge",
    "River"
  ],
  "bugs": {
    "url": "https://github.com/rowetechinc/River_electron/issues"
  },
  "homepage": "https://github.com/rowetechinc/River_electron#readme"
}
